<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gm.modules.basicconfig.dao.StarInfoDao">

    <!--获取星级信息-->
    <select id="getStarInfoPro" resultType="com.gm.modules.basicconfig.entity.StarInfoEntity">
        SELECT
        *
        FROM
        gm_star_info
        ORDER BY STAR_CODE
    </select>

</mapper>